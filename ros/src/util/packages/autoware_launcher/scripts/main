#! /usr/bin/env python

import sys

from autoware_launcher.core import AwLaunchServer
from autoware_launcher.core import AwLaunchClient
from autoware_launcher.qtui import AwGuiManager

server = AwLaunchServer(sys.argv)
client = AwLaunchClient(sys.argv, server)

guimgr = AwGuiManager(sys.argv, client)
sys.exit(guimgr.start())