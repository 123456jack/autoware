#! /usr/bin/env python

import sys

from autoware_launcher.core import AwPluginLoader
from autoware_launcher.core import AwLaunchTree
from autoware_launcher.qtui import AwGuiManager

loader = AwPluginLoader()
print("============================================================")
loader.dump_plugins()
print("============================================================")
print

launch = AwLaunchTree(loader)
print("============================================================")
launch.dump()
print("============================================================")
print

guimgr = AwGuiManager(sys.argv)
guimgr.loader = loader #temporary
sys.exit(guimgr.start_autoware_launcher(launch))