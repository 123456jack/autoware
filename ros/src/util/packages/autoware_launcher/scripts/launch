#! /usr/bin/env python

import sys

from python_qt_binding import QtCore, QtWidgets
from autoware_launcher.launch  import AwLaunchTree
from autoware_launcher.widgets import AwLaunchWidget

tree = AwLaunchTree()
tree.dump()

application = QtWidgets.QApplication(sys.argv)
window = QtWidgets.QMainWindow()
window.setCentralWidget( AwLaunchWidget(tree) )
settings = QtCore.QSettings("Autoware", "AutowareLauncher")
if settings.contains("geometry"):
    window.restoreGeometry(settings.value("geometry"))
window.show()
sys.exit(application.exec_())
