<launch>

    <arg name="transform_file"/>
    <arg name="pointsmap_file"/>
    <arg name="vectormap_file"/>

    <include file="$(arg transform_file)"/>
    <node pkg="map_file" type="points_map_loader" name="points_map_loader" args="noupdate $(arg pointsmap_file)"/>
    <node pkg="map_file" type="vector_map_loader" name="vector_map_loader" args="$(arg vectormap_file)"/>

</launch>